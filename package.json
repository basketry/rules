{
  "name": "rules",
  "version": "0.0.0",
  "description": "Base project for creating a console application in Typescript",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "clean": "run-s -s clean:*",
